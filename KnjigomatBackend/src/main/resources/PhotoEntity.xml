<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="web.books.models.entities.PhotoEntity">
        <table name="photo" schema="knjigomat" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id"/>
            </id>
            <basic name="photoUrl">
                <column name="photo_url"/>
            </basic>
            <basic name="postId">
                <column name="post_id"/>
            </basic>
        </attributes>
    </entity>
    <entity class="web.books.models.entities.ReportEntity">
        <table name="report" schema="knjigomat" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id"/>
            </id>
            <basic name="reportText">
                <column name="report_text"/>
            </basic>
            <basic name="createdTime">
                <column name="created_time"/>
            </basic>
            <basic name="userId">
                <column name="user_id"/>
            </basic>
            <basic name="postId">
                <column name="post_id"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
